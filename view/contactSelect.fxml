<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="292.0" prefWidth="429.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.ContactSelectController">
   <children>
      <Button fx:id="selectContact" layoutX="97.0" layoutY="188.0" mnemonicParsing="false" onAction="#toShowAllAppts" prefHeight="25.0" prefWidth="92.0" text="Select" />
      <Button fx:id="cancelButton" layoutX="243.0" layoutY="188.0" mnemonicParsing="false" onAction="#backToAppointments" prefHeight="25.0" prefWidth="92.0" text="Cancel" />
      <ComboBox fx:id="contactComboBox" layoutX="97.0" layoutY="91.0" prefHeight="25.0" prefWidth="238.0" />
      <Label layoutX="97.0" layoutY="30.0" text="Choose a contact to view their appointments" />
   </children>
</AnchorPane>
